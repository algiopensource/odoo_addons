<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="max_testing" model="ir.config_parameter">
            <field name="key">ci.max_testing</field>
            <field name="value">8</field>
        </record>
        <record id="max_running" model="ir.config_parameter">
            <field name="key">ci.max_running</field>
            <field name="value">17</field>
        </record>
        <record id="admin_passwd" model="ir.config_parameter">
            <field name="key">ci.admin_passwd</field>
            <field name="value">superadmin</field>
        </record>
        <record id="flake8_max_line_length" model="ir.config_parameter">
            <field name="key">ci.flake8.max_line_length</field>
            <field name="value">150</field>
        </record>
        <record id="flake8_exclude_files" model="ir.config_parameter">
            <field name="key">ci.flake8.exclude_files</field>
            <field name="value">__init__.py</field>
        </record>
        <record id="flake8_extensions" model="ir.config_parameter">
            <field name="key">ci.flake8.extensions</field>
            <field name="value">flake8-debugger flake8-print flake8-todo</field>
        </record>
        <record id="max_testing_by_branch" model="ir.config_parameter">
            <field name="key">ci.max_testing_by_branch</field>
            <field name="value">1</field>
        </record>
        <record id="max_running_by_branch" model="ir.config_parameter">
            <field name="key">ci.max_running_by_branch</field>
            <field name="value">2</field>
        </record>

    </data>
</odoo>
