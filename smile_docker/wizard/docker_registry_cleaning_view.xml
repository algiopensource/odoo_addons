<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <data noupdate="0">

    <record id="view_docker_registry_cleaning_form" model="ir.ui.view">
      <field name="name">docker.registry.cleaning.form</field>
      <field name="model">docker.registry.cleaning</field>
      <field name="arch" type="xml">
        <form string="Docker Registry Cleaning">
          <sheet>
            <span>Remove <field name="image_tag" placeholder="image:tag"/> from <field name="docker_registry_id"/></span>
          </sheet>
          <footer>
            <button name="run" type="object" string="Execute" class="btn-primary"/>
            <button special="cancel" string="Cancel" class="btn-default"/>
          </footer>
        </form>
      </field>
    </record>

  </data>
</odoo>
